{
    "name": "BudgetBuddy"
  , "version": "0.0.1"
  , "description": "A nodejs adapter to NYC budget"
  , "dependencies": {
      "express": "~3.2.3"
    , "ejs": "*"
    , "underscore": "*"
    , "underscore.string": "*"
    , "swagger-jack": "1.6.0"
    , "js-yaml": "*"
    , "moment": "*"
    , "request": "*"
    , "event-stream": "*"
    , "JSONStream": "*"
    , "nib": "*"
    , "nodemon": "*"
    , "sqlite3": "2.2.7"
  }
  , "devDependencies": {
    "mocha" : "*"
  }
  , "scripts": {
      "start": "nodemon server/server.js -p 3000"
    , "preinstall": "rm -rf data && git clone git@github.com:BetaNYC/budgetBuddyData.git data && python data/bin/csv2sqlite3.py data/processed/all.txt"
    , "test": "cake test"
  }
  , "engines": {
      "node": "0.8.14"
    , "npm": "1.1.x"
  }
  , "repository": {
      "type": "git"
    , "url": "git://github.com/betanyc/budgetBuddy.git"
  }
  , "keywords": [
      "opendata"
    , "budget"
    , "nyc"
  ]
  , "authors": ["Volkan Unsal", "Chris Whong", "John Krauss"]
  , "license": "MIT"
  , "readmeFilename": "README.md"
}
